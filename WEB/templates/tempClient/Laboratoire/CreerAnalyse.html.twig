{% extends 'tempClient/Laboratoire/acceuilLab.html.twig' %}
{% block content %}

    <section id="home">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="row homeCustom">
                    <div class="caption">
                        <div class="container">
                            {{ form_start(form,{'attr':{'novalidate':'nonvalidate'}}) }}

                                <table class="table  " style="color:black;background-color: white;margin-top: 1%">
                                    <tr> <h1>Creation d'un analyse</h1></tr>
                                    <tr>

                                        <td > {{ form_label(form.description,"Description") }}{{ form_errors(form.description)}}</td>
                                        <td>{{ form_widget(form.description,{'attr':{'class':'form-control'}})}} </td>
                                    </tr>
                                    <tr>
                                        <td >{{ form_label(form.date,"Date") }}{{ form_errors(form.date)}}</td>
                                        <td >{{ form_widget(form.date)}} </td>
                                    </tr>
                                    <tr></tr>
                                    <tr>
                                        <td class="col-lg-12 " colspan="2">{{ form_widget(form.Ajouter,{'attr':{'class':'btn btn-danger form-control'}})}} </td>

                                    </tr>
                                </table>
                            {{ form_end(form) }}

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock %}