{% extends 'tempClient/Laboratoire/acceuilLab.html.twig' %}
{% block content %}

    <section id="home">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="row homeCustom">
                    <div class="caption">
                        <div class="container">
                            <div class="col-md-12 col-sm-12">
                                <h2>Ajouter un nouveau dossier</h2>
                                <table class="table table-hover" style="color:black;background-color: white">
                                    {{ form_start(form,{'attr':{'novalidate':'novalidate'}}) }}
                                    <tr>
                                        <th> {{ form_label(form.patient,"Patient") }} {{ form_errors(form.patient) }}</th>
                                        <td class="col-lg-10">{{ form_widget(form.patient,{'attr':{'class':'form-control'}}) }} </td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">
                                            <button TYPE="submit"
                                                    class="btn btn-warning form-control">{{ button_label|default('Enregistrer') }}</button>
                                            {{ form_end(form) }}
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock %}
