<form method="post" action="{{ path('dossier_lab_delete', {'id': dossier_lab.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dossier_lab.id) }}">
    <button class="btn">Delete</button>
</form>
