{% extends 'tempAdmin/adminbase.html.twig' %}

{% block title %}Laboratoire{% endblock %}

{% block body %}
    <h1>Laboratoire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ laboratoire.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ laboratoire.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ laboratoire.roles ? laboratoire.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ laboratoire.password }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ laboratoire.Adresse }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ laboratoire.Ville }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ laboratoire.Tel }}</td>
            </tr>
            <tr>
                <th>IsVerified</th>
                <td>{{ laboratoire.isVerified ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Num_service</th>
                <td>{{ laboratoire.numService }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ laboratoire.nom }}</td>
            </tr>
            <tr>
                <th>Horaire_debut</th>
                <td>{{ laboratoire.horaireDebut ? laboratoire.horaireDebut|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Horaire_fin</th>
                <td>{{ laboratoire.horaireFin ? laboratoire.horaireFin|date('H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('laboratoire_index') }}">back to list</a>

    <a href="{{ path('laboratoire_edit', {'id': laboratoire.id}) }}">edit</a>

    {{ include('Laboratoire/_delete_form.html.twig') }}
{% endblock %}
