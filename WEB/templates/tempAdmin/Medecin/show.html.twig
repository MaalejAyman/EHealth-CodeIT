{% extends 'tempAdmin/adminbase.html.twig' %}

{% block title %}Medecin{% endblock %}

{% block body %}
    <h1>Medecin</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medecin.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ medecin.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ medecin.roles ? medecin.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ medecin.password }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ medecin.Adresse }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ medecin.Ville }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ medecin.Tel }}</td>
            </tr>
            <tr>
                <th>IsVerified</th>
                <td>{{ medecin.isVerified ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ medecin.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ medecin.prenom }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ medecin.sexe }}</td>
            </tr>
            <tr>
                <th>Horaire_debut</th>
                <td>{{ medecin.horaireDebut ? medecin.horaireDebut|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Horaire_fin</th>
                <td>{{ medecin.horaireFin ? medecin.horaireFin|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Num_service</th>
                <td>{{ medecin.numService }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('medecin_index') }}">back to list</a>

    <a href="{{ path('medecin_edit', {'id': medecin.id}) }}">edit</a>

    {{ include('tempAdmin/Medecin/_delete_form.html.twig') }}
{% endblock %}
